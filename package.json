{
    "name": "@script-development/vue-fast-table",
    "version": "1.0.8",
    "description": "Fast table for vue",
    "author": "Script",
    "scripts": {
        "build": "rollup -c scripts/config.js",
        "lint": "eslint ./src --ext .vue",
        "dev": "vue-cli-service serve --open dev/main.js",
        "docs": "typedoc src/index.ts",
        "pub": "npm publish",
        "watch": "rollup -c scripts/config.js --watch",
        "test": "mocha test/index.js",
        "test:watch": "mocha --watch test/index.js",
        "test:mutate": "stryker run"
    },
    "main": "dist/index.ssr.js",
    "module": "dist/index.esm.js",
    "files": [
        "dist",
        "types",
        "src"
    ],
    "devDependencies": {
        "@babel/core": "^7.13.10",
        "@babel/preset-env": "^7.13.12",
        "@babel/register": "^7.13.8",
        "@rollup/plugin-node-resolve": "^11.2.0",
        "@rollup/plugin-typescript": "^8.2.0",
        "@stryker-mutator/core": "^4.5.1",
        "@stryker-mutator/mocha-runner": "^4.5.1",
        "@stryker-mutator/typescript-checker": "^4.5.1",
        "@typescript-eslint/parser": "^4.19.0",
        "@vue/cli": "^4.5.12",
        "@vue/cli-plugin-babel": "^4.5.12",
        "@vue/cli-plugin-typescript": "^4.5.12",
        "@vue/cli-plugin-unit-mocha": "^4.5.12",
        "@vue/cli-service": "~4.5.0",
        "@vue/test-utils": "^1.1.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "bootstrap-vue": "^2.21.2",
        "canvas": "^2.7.0",
        "eslint": "^7.22.0",
        "eslint-plugin-mocha": "^8.1.0",
        "eslint-plugin-vue": "^7.8.0",
        "glob": "^7.1.6",
        "husky": "^5.2.0",
        "jsdom": "^16.5.1",
        "mocha": "^8.3.2",
        "nyc": "^15.1.0",
        "postcss": "^8.2.8",
        "prettier": "^2.2.1",
        "pretty-quick": "^3.1.0",
        "rollup": "^2.42.3",
        "rollup-plugin-typescript2": "^0.30.0",
        "rollup-plugin-vue": "^5.1.9",
        "sinon": "^10.0.0",
        "typedoc": "^0.20.33",
        "typescript": "^4.2.3",
        "vue": "^2.6.12",
        "vue-template-compiler": "^2.6.11",
        "webpack": "^4.46.0",
        "webpack-cli": "^4.5.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged --pattern \"**/*.*(js|json)\""
        }
    },
    "license": "MIT",
    "nyc": {
        "reporter": [
            "lcov",
            "text"
        ]
    },
    "typings": "dist/index.d.ts"
}
